<view class="stock-container f28"><view hidden="{{!(isShow)}}" class="login-dialog"><view class="login-dialog-cont"><view class="login-list"><text class="login-txt">发动机编码</text><input class="uni-input login-input" placeholder="请录入发动机编码" data-event-opts="{{[['input',[['__set_model',['','engineCode','$event',[]]]]]]}}" value="{{engineCode}}" bindinput="__e"/></view><view class="ub"><button class="login-dialog-btn" style="background-color:#CCCCCC;color:#000000;" type="primary" data-event-opts="{{[['tap',[['cancel']]]]}}" bindtap="__e">取消</button><button class="login-dialog-btn" type="primary" data-event-opts="{{[['tap',[['submitAgent']]]]}}" bindtap="__e">提交</button></view></view></view><view class="custpro-main search-cont"><view class="index-search"><view class="index-search-cont"><view class="search-input-box" style="width:100%;"><input class="index-search-txt" confirm-type="search" placeholder="请输入产品编号或规格" data-event-opts="{{[['input',[['__set_model',['','keyTxt','$event',[]]],['getList',['$event']]]],['confirm',[['toSearch']]]]}}" value="{{keyTxt}}" bindinput="__e" bindconfirm="__e"/><button data-event-opts="{{[['tap',[['toSearch']]]]}}" class="index-search-btn s-btn" bindtap="__e">搜索</button></view></view></view><block wx:for="{{stockList}}" wx:for-item="list" wx:for-index="__i0__"><view class="ub ub-ver am-list-item"><view class="ub ub-ac"><view class="ub ub-ver ub-f1"><view class="ub"><text class="ub ub-f1 am-blod">{{list.spec}}</text></view><view class="ub"><text data-event-opts="{{[['tap',[['flod',['$0'],[[['stockList','',__i0__]]]]]]]}}" class="ub ub-f1 am-text-primary" bindtap="__e">查看识别码</text><text class="ub ub-f1">{{"库存:"+list.qty}}</text></view></view><text data-event-opts="{{[['tap',[['showTop',['$0'],[[['stockList','',__i0__]]]]]]]}}" class="zhiding" style="{{'background-color:'+(list.t_top==1?'#CCCCCC':'#d44242')+';'}}" bindtap="__e">{{list.t_top==1?"取消置顶":"置顶"}}</text></view><view class="ub"><view hidden="{{!(list.isShow)}}" class="code-list"><block wx:for="{{list.item}}" wx:for-item="items" wx:for-index="__i1__"><block><view class="code-list-item">{{items.motorid}}</view></block></block></view></view></view></block></view><view class="ub am-btn-fixed am-bg-white ub-ac"><view class="ub ub-f1" style="padding-left:30rpx;">{{"总计："+stockTotal}}</view><view class="ub"><button class="am-btn" type="primary" data-event-opts="{{[['tap',[['scanEnter']]]]}}" bindtap="__e">扫描入库</button><button class="am-btn" type="warn" data-event-opts="{{[['tap',[['handEnter']]]]}}" bindtap="__e">手动入库</button></view></view><view hidden="{{!(tiShi)}}" class="tiShiWrap"><view class="tiShiWrapCon">{{tiShiText}}</view></view></view>